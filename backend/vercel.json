{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/test-db.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/test-db",
            "dest": "/api/test-db.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}